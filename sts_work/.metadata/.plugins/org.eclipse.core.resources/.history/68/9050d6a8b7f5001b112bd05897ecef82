<script>
function check(){
	if(document.loginFrom.id.value==""){
		alert('아이디를 입력하시오');	
		document.loginForm.id.focus();
		return false;
	}
	if(document.loginFrom.pw.value==""){
		alert('비밀번호를 입력하시오');	
		document.loginForm.pw.focus();
		return false;
	}
	return true;
}
</script>
